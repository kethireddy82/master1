    <aura:attribute name="age1" type="String" default=""/>
    <aura:attribute name="serviceAffiliationForOther" type="String" default=""/>
    <aura:attribute name="serviceStatusForOther" type="String" default=""/>
    <aura:attribute name="complainantServiceStatus" type="String" default=""/>
    <aura:attribute name="sponsorBatch" type="String" default=""/>
    <aura:attribute name="complainantPayGrade" type="String" default=""/>
